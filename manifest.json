{
  "name": "mch-battle-uid",
  "description": "mch-battle-uid extension",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "host_permissions": ["https://www.mycryptoheroes.net/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://www.mycryptoheroes.net/battles/**"],
      "css": ["float-window.css"]
    }
  ]
}
